<h1>Listing subscribers</h1>

<table>
  <thead>
    <tr>
      <th>Benefit status</th>
      <th>Benefits</th>
      <th>Birth date</th>
      <th>Contacts</th>
      <th>Eligibility begin date</th>
      <th>Employment status</th>
      <th>First name</th>
      <th>Gender</th>
      <th>Group or policy number</th>
      <th>Handicapped</th>
      <th>Last name</th>
      <th>Maintenance reason</th>
      <th>Maintenance type</th>
      <th>Member</th>
      <th>Middle name</th>
      <th>Relationship</th>
      <th>Ssn</th>
      <th>Subscriber number</th>
      <th>Substance abuse</th>
      <th>Tobacco use</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @subscribers.each do |subscriber| %>
      <tr>
        <td><%= subscriber.benefit_status %></td>
        <td><%= subscriber.benefits %></td>
        <td><%= subscriber.birth_date %></td>
        <td><%= subscriber.contacts %></td>
        <td><%= subscriber.eligibility_begin_date %></td>
        <td><%= subscriber.employment_status %></td>
        <td><%= subscriber.first_name %></td>
        <td><%= subscriber.gender %></td>
        <td><%= subscriber.group_or_policy_number %></td>
        <td><%= subscriber.handicapped %></td>
        <td><%= subscriber.last_name %></td>
        <td><%= subscriber.maintenance_reason %></td>
        <td><%= subscriber.maintenance_type %></td>
        <td><%= subscriber.member_id %></td>
        <td><%= subscriber.middle_name %></td>
        <td><%= subscriber.relationship %></td>
        <td><%= subscriber.ssn %></td>
        <td><%= subscriber.subscriber_number %></td>
        <td><%= subscriber.substance_abuse %></td>
        <td><%= subscriber.tobacco_use %></td>
        <td><%= link_to 'Show', subscriber %></td>
        <td><%= link_to 'Edit', edit_subscriber_path(subscriber) %></td>
        <td><%= link_to 'Destroy', subscriber, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Subscriber', new_subscriber_path %>
